<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="wheelchair">
    <pose>0 0 0.1 0 0 0</pose>
    <static>false</static>
    <link name="Mainbody">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass>0.126</mass>
        <inertia> <!-- inertias are tricky to compute -->
          <!-- http://gazebosim.org/tutorials?tut=inertia&cat=build_robot -->
          <ixx>12.10</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0.03</ixy>         <!-- for a box: ixy = 0 -->
          <ixz>2.70</ixz>         <!-- for a box: ixz = 0 -->
          <iyy>13.0</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0.10</iyz>         <!-- for a box: iyz = 0 -->
          <izz>9.0</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://wheelchair/meshes/Wheelchair.stl</uri>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://wheelchair/meshes/Wheelchair.stl</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
